   <select id="selectListByClassId" parameterClass="com.linklife.domain.ibator.NuistChat" resultClass="com.linklife.domain.model.NuistChatModel">
		<![CDATA[
		select a.chat_id as chatId, a.content, a.username, a.add_time as addTime
		from nuist_chat a
		where a.class_id=#classId:INTEGER# AND a.parent_id=0 AND a.content!=""
		ORDER BY a.add_time
		DESC 
		LIMIT 20
		]]>
	</select>
	
	<select id="selectListByClassIdAndParentId" parameterClass="com.linklife.domain.ibator.NuistChat" resultClass="com.linklife.domain.model.NuistChatModel">
		<![CDATA[
		select a.chat_id as chatId, a.parent_id as parentId, a.username, a.content, a.add_time as addTime
		from linklife.nuist_chat a 
		where a.class_id=#classId:INTEGER# AND a.parent_id IN 
		(select t.chat_id from
		(select b.chat_id
		from linklife.nuist_chat b 
		where b.class_id=#classId:INTEGER# AND b.parent_id=0 AND b.content!=""
		ORDER BY b.add_time
		DESC 
		LIMIT 20)
		 as t ) AND a.content!=""
		]]>
	</select>
